<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tawsilatn">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activities.InformationActivity"></activity>
        <activity
            android:name=".Activities.TawsilaHomePage"
            android:label="@string/title_activity_tawsila_home_page"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Activities.RegisterActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Activities.LoginPage"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Activities.GetStarted"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>